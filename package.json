{
  "name": "temp-email-guard",
  "version": "1.0.0",
  "description": "A powerful TypeScript package to detect and guard against temporary email addresses",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths && tsc --emitDeclarationOnly --outDir dist",
    "build:watch": "swc src -d dist --strip-leading-paths --watch",
    "aggregate": "ts-node scripts/aggregate-domains.ts",
    "aggregate:build": "npm run build && node dist/scripts/aggregate-domains.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:benchmark": "jest __tests__/benchmark.test.ts",
    "benchmark": "npm run build && node benchmark.js",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "email",
    "validation",
    "temp-email",
    "temporary-email",
    "email-guard",
    "spam-prevention",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "data",
    "README.md"
  ],
  "devDependencies": {
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
